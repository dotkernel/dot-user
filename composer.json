{
  "name": "dotkernel/dot-user",
  "type": "library",
  "description": "Complete configurable user module for DK built around dot-authentication-web",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "psr/http-message": "^1.0",
    "container-interop/container-interop": "^1.1",
    "zendframework/zend-expressive-template": "^1.0",
    "zendframework/zend-diactoros": "^1.3",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-db": "^2.8",
    "zendframework/zend-math": "^3.0",
    "zendframework/zend-crypt": "^3.1",

    "dotkernel/dot-helpers": "0.6.x-dev",
    "dotkernel/dot-ems": "dev-develop as 0.6.x-dev",
    "dotkernel/dot-filter": "0.6.x-dev",
    "dotkernel/dot-validator": "0.6.x-dev",
    "dotkernel/dot-inputfilter": "0.6.x-dev",
    "dotkernel/dot-hydrator": "0.6.x-dev",
    "dotkernel/dot-form": "0.6.x-dev",
    "dotkernel/dot-event": "0.6.x-dev",
    "dotkernel/dot-flashmessenger": "0.6.x-dev",
    "dotkernel/dot-controller": "0.6.x-dev",
    "dotkernel/dot-controller-plugin-flashmessenger": "dev-develop as 0.6.x-dev",
    "dotkernel/dot-controller-plugin-authentication": "0.6.x-dev",
    "dotkernel/dot-controller-plugin-authorization": "0.6.x-dev",
    "dotkernel/dot-controller-plugin-forms": "0.6.x-dev",
    "dotkernel/dot-authentication": "0.6.x-dev",
    "dotkernel/dot-authentication-web": "0.6.x-dev",
    "dotkernel/dot-authorization": "0.6.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-ems"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-form"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-inputfilter"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-filter"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-validator"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-hydrator"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-controller-plugin-forms"
    }
  ],
  "autoload": {
    "psr-4": {
      "Dot\\User\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\User\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6-dev"
    }
  }
}
